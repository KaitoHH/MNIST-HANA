{
  "exposed": true,
  "authentication": {
   "method": "Form"
  },
  "cache_control": "must-revalidate",
  "cors": {
   "enabled": true,
   "allowMethods": [
   "GET",
   "POST"
   ],
   "allowOrigin": ["*"],
   "allowHeaders":["Content-Type","authorization","accept"]
  },
  "enable_etags": false,
  "force_ssl": false,
  "prevent_xsrf": false,
  "rewrite_rules": [
        {
            "source": "/api/show",
            "target": "/getResult.xsjs"
        },
        {
        	"source": "/api/list",
        	"target": "/list.xsodata/mnist.DDL.knn::KNN_TEST"
        },
        {
        	"source": "/api/upload",
        	"target": "/list.xsodata/mnist.DDL.knn::KNN_MAN"
        }
    ]
}